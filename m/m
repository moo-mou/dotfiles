#!/usr/local/bin/m_venv/bin/python
import fire

from m_docker import Docker
from m_deploy import Deploy
from m_chef import Chef
from m_www import WWW

class M2:
    def __init__(self):
        self.docker = Docker()
        self.chef = Chef()
        self.deploy = Deploy()
        self.www = WWW()

if __name__ == '__main__':
    fire.Fire(M2())
