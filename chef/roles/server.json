{
  "nginx": {
    "domains": [
      {
        "root_url": "ohsloth.com",
        "servers": [
          {
            "name": "lab",
            "upstream": "lab"
          },
          {
            "name": "api",
            "upstream": "api"
          },
          {
            "locations": [
              {
                "path": "/dist",
                "root": "/var/app/ohsloth"
              }
            ],
            "name": "app",
            "upstream": "app"
          },
          {
            "name": "www",
            "root": "/var/www/ohsloth"
          }
        ],
        "subdomains": [
          "api",
          "app",
          "lab",
          "www"
        ],
        "upstreams": [
          {
            "address": "localhost:3000",
            "name": "api"
          },
          {
            "address": "localhost:8080",
            "name": "svc"
          },
          {
            "address": "localhost:8888",
            "name": "lab"
          }
        ]
      }
    ],
    "user": "me+letsencrypt@moomou.com"
  },
  "run_list": [
    "recipe[server::default]",
    "recipe[supervisor_job::default]",
    "recipe[nginx::default]"
  ],
  "server": {
    "username": "moomou",
    "users": [
      {
        "username": "moomou"
      }
    ]
  },
  "supervisor_job": {
    "configs": []
  }
}
